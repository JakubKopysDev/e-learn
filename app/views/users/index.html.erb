<%= render 'shared/header' %>
<div id="user_index">
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= link_to user do %>
        <%= user.username %>
        <%= user.email %>
        <div class="authorized_links">
            <% if current_user.admin? && !user.admin?%>
              <%= link_to "Delete", user_path(user), method: :delete, data: { confim: "Are you sure?"} %>
            <% end %>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>
